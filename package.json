{
  "name": "ttv-extension-boilerplate",
  "version": "1.0.0",
  "description": "My TTV Extension Boilerplate",
  "scripts": {
    "test": "jest",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "http": "npx http-server"
  },
  "author": "ctnava.github.io",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "jest": "^28.1.0",
    "react-test-renderer": "^18.1.0",
    "sinon": "^14.0.0",
    "webpack": "^5.72.1"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "jose": "^4.8.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-bootstrap": "^2.2.3",
    "react-dom": "^18.1.0",
    "react-dotenv": "^0.1.3",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.3.2",
    "react-tooltip": "^4.2.21",
    "toastr": "^2.1.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(scss|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/file.js",
      "\\.(css|sccss|less)$": "<rootDir>/mocks/style.js"
    }
  }
}
